{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "indent": "off",
        "linebreak-style": "off",
        "comma-dangle": "off",
        "arrow-parens": "off",
        "no-unused-vars": "warn",
        "eol-last": "off",
        "require-jsdoc": "off"
    },
    "extends": [
        "eslint:recommended",
        "google"
    ]
}